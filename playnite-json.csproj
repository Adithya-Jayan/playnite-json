<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>playnite-json</AssemblyName>
    <RootNamespace>playnite_json</RootNamespace>
    <Product>Playnite JSON Export</Product>
    <AssemblyTitle>Playnite JSON Export</AssemblyTitle>
    <Authors>Adithya-Jayan</Authors>
    <Description>Export Playnite library to JSON/Zip</Description>
    <Copyright>Copyright Â© 2024</Copyright>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="PlayniteSDK" Version="6.11.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="WindowsBase" />
    <Reference Include="System.IO.Compression" />
    <Reference Include="System.IO.Compression.FileSystem" />
  </ItemGroup>

</Project>